.switch_ricamo {
    position: relative;
    top: 159%;
    left: -205%;
    width: 265px;
    height: 50px;
    text-align: center;
    margin: -30px 0 0 -75px;
    background: #023047;
    transition: all 0.2s ease;
    border-radius: 25px;
}
.switch_ricamo span {
    position: absolute;
    width: 20px;
    height: 4px;
    top: 50%;
    left: 46%;
    margin: -2px 0px 0px -4px;
    background: #fff;
    display: block;
    transform: rotate(-45deg);
    transition: all 0.2s ease;
}
.switch_ricamo span:after {
    content: "";
    display: block;
    position: absolute;
    width: 4px;
    height: 12px;
    margin-top: -8px;
    background: #fff;
    transition: all 0.2s ease;
}
input[type=radio] {
    display: none;
}
.switch_ricamo label {
    cursor: pointer;
  /*   color: rgba(0,0,0,0.2); */
    color: #eee;
    width: 60px;
    line-height: 50px;
    transition: all 0.2s ease;
}
label[for=yes] {
    position: absolute;
    left: 33px;
    height: 20px;
}
label[for=no] {
    position: absolute;
    right: 56px;
}
#no:checked ~ .switch_ricamo {
    background: #fff;
    opacity: 0.8;
    border: 3px solid #023047;

}
#no:checked ~ .switch_ricamo span {
    background: #023047;
    margin-left: -8px;
   
}
#no:checked ~ .switch_ricamo span:after {
    background: #023047;
    height: 20px;
    margin-top: -8px;
    margin-left: 8px;
}
 #yes:checked ~ .switch_ricamo label[for=yes] {
    color: #fff;
    opacity: 0.4;
}
#no:checked ~ .switch_ricamo label[for=no] {
    color: #023047;
    opacity: 0.4;
} 
#yes:checked ~ .switch_ricamo label {
    color: #fff;
}
#no:checked ~ .switch_ricamo label,
#no:checked ~ .switch_ricamo span:after
 {
    color: #023047;
}
